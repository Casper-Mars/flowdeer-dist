{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "FlowDeer Tree",
  "version": "3.0.1",
  "identifier": "com.ft07.flowdeer.tree",
  "build": {
    "frontendDist": "../../docs"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "FlowDeer Tree",
        "width": 1000,
        "height": 900
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
